# Aliases
source $ZDOTDIR/.zsh_aliases

# History
source $ZDOTDIR/.zsh_history


# Keybinding
source $ZDOTDIR/.zsh_bindkey

# Completion
source $ZDOTDIR/.zsh_completions

# Prompt
source $ZDOTDIR/plugins/zsh-git-prompt/gitstatus.plugin.zsh
